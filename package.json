    "babel-preset-stage-3": "^6.22.0",
    "css-loader": "^0.27.3",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.3",
